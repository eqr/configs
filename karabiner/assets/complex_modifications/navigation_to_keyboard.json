{
  "title": "Navigation to keyboard, place it to ~/.config/karabiner/assets/complex_modifications",
   "rules": [   
    { "description": "Fn + I/J/K/L to Arrow Keys",
      "manipulators": [{"type": "basic", "from": {"key_code": "i", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "up_arrow"}] },
                       {"type": "basic", "from": {"key_code": "j", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "left_arrow"}]},
                       {"type": "basic", "from": {"key_code": "k", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "down_arrow"}]},
                       {"type": "basic", "from": {"key_code": "l", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "right_arrow"}]},
                       {"type": "basic", "from": {"key_code": "u", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "home"}]},
                       {"type": "basic", "from": {"key_code": "o", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "end"}]},
                       {"type": "basic", "from": {"key_code": "h", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "page_down"}]},
                       {"type": "basic", "from": {"key_code": "y", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}}, "to": [{"key_code": "page_up"}]}] }
  ]
}